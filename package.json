{
  "name": "kismet-react",
  "homepage": "https://kismet-ux.com",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "// bootstrap scripts": "",
    "bootstrap": "yarn install && yarn build:packages",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "start": "vite",
    "gql-codegen": "graphql-codegen --require dotenv/config --config gql-codegen-config.ts",
    "to-env": "switch-env",
    "to-personal-dev": "switch-env personal-dev",
    "// clean scripts": "",
    "clean": "rm -rf node_modules build .env",
    "clean:cache": "rm -rf node_modules/.vite",
    "clean:generated-types": "rm -rf src/shared/types/graphql/__generated__"
  },
  "dependencies": {
    "@apollo/client": "3.11.8",
    "clsx": "^2.1.1",
    "exceljs": "4.4.0",
    "file-saver": "2.0.5",
    "final-form": "4.20.10",
    "final-form-arrays": "3.1.0",
    "graphql": "16.9.0",
    "graphql-tag": "2.12.6",
    "jszip": "3.10.1",
    "lodash": "4.17.21",
    "luxon": "3.5.0",
    "nanoid": "5.0.8",
    "notistack": "3.0.1",
    "phonumber": "1.0.12",
    "qs": "6.13.0",
    "react": "18.3.1",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.3.1",
    "react-dropzone": "14.3.5",
    "react-final-form": "6.5.9",
    "react-final-form-arrays": "3.1.4",
    "react-google-autocomplete": "2.7.3",
    "react-imask": "7.6.1",
    "react-number-format": "5.4.2",
    "react-router-dom": "6.27.0",
    "react-tooltip": "^5.28.0",
    "react-virtuoso": "4.12.0",
    "readable-web-to-node-stream": "3.0.2",
    "recharts": "2.13.3",
    "scroll-snap": "^4.0.5",
    "use-debounce": "10.0.4",
    "use-react-router-breadcrumbs": "^4.0.1",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/file-saver": "2.0.7",
    "@types/google.maps": "3.58.1",
    "@types/lodash": "4.17.13",
    "@types/luxon": "3.4.2",
    "@types/node": "22.8.7",
    "@types/qs": "6.9.16",
    "@types/react": "18.3.12",
    "@types/react-copy-to-clipboard": "5.0.7",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.13.0",
    "@typescript-eslint/parser": "8.13.0",
    "@vitejs/plugin-react-swc": "3.7.1",
    "ci-info": "4.0.0",
    "eslint": "9.14.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-promise": "7.1.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "prettier": "3.3.3",
    "sass": "^1.80.6",
    "stylelint": "^16.10.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "typescript": "5.6.3",
    "vite": "5.4.6",
    "vite-plugin-checker": "0.8.0",
    "vite-plugin-node-polyfills": "0.22.0",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "2.1.4"
  }
}
